# EditorConfig is awesome: https://editorconfig.org

# top-most EditorConfig file
root = true
indent_style = space
indent_size = 4

end_of_line = lf
insert_final_newline = true
charset = utf-8

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,py,php,twig}]
charset = utf-8